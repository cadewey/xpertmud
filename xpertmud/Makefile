	$(INSTALL_DATA) $(srcdir)/lo16-app-xpertmud.png $(DESTDIR)$(kde_icondir)/locolor/16x16/apps/xpertmud.png
	$(mkinstalldirs) $(DESTDIR)$(kde_icondir)/hicolor/16x16/apps
