force-reedit:
		cd $(top_srcdir) && \
	  $(AUTOMAKE) --gnu  xpertmud/Makefile
	cd $(top_srcdir) && perl admin/am_edit xpertmud/Makefile.in


#>+ 21
clean-bcheck: 
	rm -f *.bchecktest.cc *.bchecktest.cc.class a.out

bcheck: bcheck-recursive

bcheck-am:
	@for i in ; do \
	    if test $(srcdir)/$$i -nt $$i.bchecktest.cc; then \
	        echo "int main() {return 0;}" > $$i.bchecktest.cc ; \
	        echo "#include \"$$i\"" >> $$i.bchecktest.cc ; \
	        echo "$$i"; \
	        if ! $(CXXCOMPILE)  --dump-class-hierarchy -c $$i.bchecktest.cc; then \
	            rm -f $$i.bchecktest.cc; exit 1; \
	        fi ; \
	        echo "" >> $$i.bchecktest.cc.class; \
	        perl $(top_srcdir)/admin/bcheck.pl $$i.bchecktest.cc.class || { rm -f $$i.bchecktest.cc; exit 1; }; \
	        rm -f a.out; \
	    fi ; \
	done


#>+ 11
xpertmud.all_cc.cc: $(srcdir)/Makefile.in $(srcdir)/MCCP.cc $(srcdir)/TextBufferHistoryView.cc $(srcdir)/main.cc $(srcdir)/xpertmud.cc $(srcdir)/xpertmud_scripting.cc $(srcdir)/mxinputline.cc $(srcdir)/PluginWrapper.cc $(srcdir)/configdialog.cc $(srcdir)/InputAction.cc $(srcdir)/InputStreamHandler.cc $(srcdir)/TelnetFilter.cc $(srcdir)/Connection.cc $(srcdir)/ConnectionHandler.cc $(srcdir)/IacHandlerCollection.cc $(srcdir)/CtrlParser.cc $(srcdir)/Bookmark.cc $(srcdir)/BookmarkEditor.cc $(srcdir)/TextBuffer.cc $(srcdir)/TextBufferView.cc $(srcdir)/LogToFile.cc $(srcdir)/DCOPxpertmud_skel.cc $(srcdir)/TextBuffer.moc.cc $(srcdir)/BookmarkEditor.moc.cc $(srcdir)/configdialog.moc.cc $(srcdir)/TextBufferHistoryView.moc.cc $(srcdir)/IacHandlerCollection.moc.cc $(srcdir)/mxinputline.moc.cc $(srcdir)/PluginWrapper.moc.cc $(srcdir)/Connection.moc.cc $(srcdir)/ConnectionHandler.moc.cc $(srcdir)/Bookmark.moc.cc $(srcdir)/TextBufferView.moc.cc $(srcdir)/InputAction.moc.cc $(srcdir)/MCCP.moc.cc $(srcdir)/xpertmud.moc.cc 
	@echo 'creating xpertmud.all_cc.cc ...'; \
	rm -f xpertmud.all_cc.files xpertmud.all_cc.final; \
	echo "#define KDE_USE_FINAL 1" >> xpertmud.all_cc.final; \
	for file in MCCP.cc TextBufferHistoryView.cc main.cc xpertmud.cc xpertmud_scripting.cc mxinputline.cc PluginWrapper.cc configdialog.cc InputAction.cc InputStreamHandler.cc TelnetFilter.cc Connection.cc ConnectionHandler.cc IacHandlerCollection.cc CtrlParser.cc Bookmark.cc BookmarkEditor.cc TextBuffer.cc TextBufferView.cc LogToFile.cc DCOPxpertmud_skel.cc TextBuffer.moc.cc BookmarkEditor.moc.cc configdialog.moc.cc TextBufferHistoryView.moc.cc IacHandlerCollection.moc.cc mxinputline.moc.cc PluginWrapper.moc.cc Connection.moc.cc ConnectionHandler.moc.cc Bookmark.moc.cc TextBufferView.moc.cc InputAction.moc.cc MCCP.moc.cc xpertmud.moc.cc ; do \
	  echo "#include \"$$file\"" >> xpertmud.all_cc.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> xpertmud.all_cc.final; \
	done; \
	cat xpertmud.all_cc.final xpertmud.all_cc.files > xpertmud.all_cc.cc; \
	rm -f xpertmud.all_cc.final xpertmud.all_cc.files

