#>+ 21
clean-bcheck: 
	rm -f *.bchecktest.cc *.bchecktest.cc.class a.out

bcheck: bcheck-am

bcheck-am:
	@for i in ; do \
	    if test $(srcdir)/$$i -nt $$i.bchecktest.cc; then \
	        echo "int main() {return 0;}" > $$i.bchecktest.cc ; \
	        echo "#include \"$$i\"" >> $$i.bchecktest.cc ; \
	        echo "$$i"; \
	        if ! $(CXXCOMPILE)  --dump-class-hierarchy -c $$i.bchecktest.cc; then \
	            rm -f $$i.bchecktest.cc; exit 1; \
	        fi ; \
	        echo "" >> $$i.bchecktest.cc.class; \
	        perl $(top_srcdir)/admin/bcheck.pl $$i.bchecktest.cc.class || { rm -f $$i.bchecktest.cc; exit 1; }; \
	        rm -f a.out; \
	    fi ; \
	done


#>+ 11
libxmperlinterpreter_la.all_cc.cc: $(srcdir)/Makefile.in $(srcdir)/AutoGuiScriptingWrapper.cc $(srcdir)/XMPerl.cc $(srcdir)/XMPerlFactory.cc $(srcdir)/XMPerl.moc.cc $(srcdir)/XMPerlFactory.moc.cc 
	@echo 'creating libxmperlinterpreter_la.all_cc.cc ...'; \
	rm -f libxmperlinterpreter_la.all_cc.files libxmperlinterpreter_la.all_cc.final; \
	echo "#define KDE_USE_FINAL 1" >> libxmperlinterpreter_la.all_cc.final; \
	for file in AutoGuiScriptingWrapper.cc XMPerl.cc XMPerlFactory.cc XMPerl.moc.cc XMPerlFactory.moc.cc ; do \
	  echo "#include \"$$file\"" >> libxmperlinterpreter_la.all_cc.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> libxmperlinterpreter_la.all_cc.final; \
	done; \
	cat libxmperlinterpreter_la.all_cc.final libxmperlinterpreter_la.all_cc.files > libxmperlinterpreter_la.all_cc.cc; \
	rm -f libxmperlinterpreter_la.all_cc.final libxmperlinterpreter_la.all_cc.files

