libxmperlinterpreter.la.closure: $(libxmperlinterpreter_la_OBJECTS) $(libxmperlinterpreter_la_DEPENDENCIES)
	@echo "int main() {return 0;}" > libxmperlinterpreter_la_closure.cc
	@$(LTCXXCOMPILE) -c libxmperlinterpreter_la_closure.cc
	$(CXXLINK) libxmperlinterpreter_la_closure.lo $(libxmperlinterpreter_la_LDFLAGS) $(libxmperlinterpreter_la_OBJECTS) $(libxmperlinterpreter_la_LIBADD) $(LIBS)
	@rm -f libxmperlinterpreter_la_closure.* libxmperlinterpreter.la.closure
	@echo "timestamp" > libxmperlinterpreter.la.closure


